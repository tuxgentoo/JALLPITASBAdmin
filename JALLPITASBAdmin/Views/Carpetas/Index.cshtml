@model IEnumerable<JALLPITASBAdmin.Models.Carpeta>
<!-- Content Row -->
@*<div class="row">*@
<div class="card shadow mb-4">
    <div class="card-header py-3 bg-gradient-primary">
        <h6 class="m-0 font-weight-bold text-white">Lista de procesos</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead class="bg-gradient-primary text-white">
                    <tr>
                        <th>IDCarpeta</th>
                        <th>Agrupacion Social</th>
                        <th>Cuerpos</th>
                        <th>Fojas</th>
                        <th>Poligono</th>
                        <th>Fecha</th>
                        <th>Observaciones</th>
                    </tr>
                </thead>
                <tfoot class="bg-gradient-primary text-white">
                    <tr>
                        <th>IDCarpeta</th>
                        <th>Agrupacion Social</th>
                        <th>Cuerpos</th>
                        <th>Fojas</th>
                        <th>Poligono</th>
                        <th>Fecha</th>
                        <th>Observaciones</th>
                    </tr>
                </tfoot>
                <tbody>                   
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>                                
                                <a asp-action="Edit" asp-route-id="@item.CarpetaId">@Html.DisplayFor(modelItem => item.IDCarpeta)</a>
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.AgrupacionSocial)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Cuerpos)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Fojas)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Poligono)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.FechaRegistro)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Observaciones)
                            </td>
                            @*<td>
                                <a asp-action="Edit" asp-route-id="@item.CarpetaId">Edit</a> |
                                <a asp-action="Details" asp-route-id="@item.CarpetaId">Details</a> |
                                <a asp-action="Delete" asp-route-id="@item.CarpetaId">Delete</a>
                            </td>*@
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>